{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(./gradlew:*)",
      "Bash(find:*)",
      "Bash(mkdir:*)",
      "Bash(grep:*)",
      "Bash(timeout:*)",
      "Bash(.gradlew :shared:test:*)",
      "Bash(kotlin debug_test.kt)",
      "Bash(java:*)",
      "Bash(rm:*)",
      "Bash(.gradlew :oss:oss-shared:test --tests \"*OssPropertiesTest*toString*\")"
    ],
    "deny": [
      "Bash(node:*)",
      "Bash(npx:*)",
      "Bash(yarn:*)",
      "Bash(npm:*)",
      "Bash(git:*)"
    ]
  }
}
